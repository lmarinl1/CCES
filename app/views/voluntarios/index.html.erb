<p id="notice"><%= notice %></p>

<h1>Voluntarios</h1>

<table>
  <thead>
    <tr>
      <th>Cedula</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Nacimiento</th>
      <th>Perfil</th>
      <th>Email</th>
      <th>Creador</th>
      <th>Activo</th>
      <th>Modificador</th>
      <th>Cel</th>
      <th>Zona</th>
      <th>Municipio</th>
      <th>Instagram</th>
      <th>Facebook</th>
      <th>Twitter</th>
      <th>Whatsapp</th>
      <th>Descripcion</th>
      <th>Direccion</th>
      <th>Sexo</th>
      <th>Puntov</th>
      <th>Empresa</th>
      <th>Cargo</th>
      <th>Universidad</th>
      <th>Carrera</th>
      <th>Votos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voluntarios.each do |voluntario| %>
      <tr>
        <td><%= voluntario.cedula %></td>
        <td><%= voluntario.nombre %></td>
        <td><%= voluntario.apellido %></td>
        <td><%= voluntario.nacimiento %></td>
        <td><%= voluntario.perfil %></td>
        <td><%= voluntario.email %></td>
        <td><%= voluntario.creador %></td>
        <td><%= voluntario.activo %></td>
        <td><%= voluntario.modificador %></td>
        <td><%= voluntario.cel %></td>
        <td><%= voluntario.zona %></td>
        <td><%= voluntario.municipio %></td>
        <td><%= voluntario.instagram %></td>
        <td><%= voluntario.facebook %></td>
        <td><%= voluntario.twitter %></td>
        <td><%= voluntario.whatsapp %></td>
        <td><%= voluntario.descripcion %></td>
        <td><%= voluntario.direccion %></td>
        <td><%= voluntario.sexo %></td>
        <td><%= voluntario.puntov %></td>
        <td><%= voluntario.empresa %></td>
        <td><%= voluntario.cargo %></td>
        <td><%= voluntario.universidad %></td>
        <td><%= voluntario.carrera %></td>
        <td><%= voluntario.votos %></td>
        <td><%= link_to 'Show', voluntario %></td>
        <td><%= link_to 'Edit', edit_voluntario_path(voluntario) %></td>
        <td><%= link_to 'Destroy', voluntario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voluntario', new_voluntario_path %>
